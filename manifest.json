{
  "manifest_version": 3,
  "name": "Anti Carnism Chrom Extension",
  "description": "Holding non-vegans accountable for supporting animal abuse",
  "version": "0.0.0.1",
  "icons": {
    "128": "images/endSpeciesism.png",
    "16": "images/endSpeciesism.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*.jpeg", "images/*.png", "images/*.jpg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
